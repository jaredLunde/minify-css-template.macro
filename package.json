{
  "name": "minify-css-template.macro",
  "version": "1.0.3",
  "homepage": "https://github.com/jaredLunde/minify-css-template.macro#readme",
  "repository": "github:jaredLunde/minify-css-template.macro",
  "bugs": "https://github.com/jaredLunde/minify-css-template.macro/issues",
  "author": "Jared Lunde <jared@BeStellar.co> (https://jaredLunde.com)",
  "license": "MIT",
  "description": "A babel plugin for removing the whitespace from a CSS-generating tagged template literal while preserving functions/variables instances",
  "keywords": [
    "minify css template.macro"
  ],
  "main": "dist/cjs/macro.js",
  "module": "dist/es/macro.js",
  "files": [
    "/dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "babel src -d dist/cjs -x .js --ignore \"**/*.test.js\",\"**/test.js\" --delete-dir-on-start",
    "build:es": "cross-env BABEL_ENV=es babel src -d dist/es -x .js --ignore \"**/*.test.js\",\"**/test.js\" --delete-dir-on-start",
    "format": "prettier --write \"**/*.{js,md,yml,json,babelrc,eslintrc,prettierrc}\"",
    "lint": "eslint .",
    "prepublishOnly": "npm run lint && npm run test && npm run build && npm run format",
    "test": "jest",
    "validate": "npm run lint && npm run test -- --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint",
      "prettier --write"
    ],
    "**/*.{md,yml,json,babelrc,eslintrc,prettierrc}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@lunde/babel-preset-es": "latest",
    "babel-eslint": "latest",
    "babel-plugin-tester": "^7.0.1",
    "cross-env": "latest",
    "eslint": "latest",
    "eslint-import-resolver-jest": "latest",
    "eslint-plugin-jest": "latest",
    "husky": "latest",
    "jest": "latest",
    "lint-staged": "latest",
    "prettier": "latest",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "@babel/generator": "^7.6.4",
    "babel-literal-to-ast": "^2.1.0",
    "babel-plugin-macros": "^2.6.1"
  },
  "peerDependencies": {}
}
